{
    "name": "jeffersonsimaogoncalves/laravel-device-tracking",
    "description": "a library that allow to track different devices used",
    "keywords": [
        "ivanomatteo",
        "laravel-device-tracking"
    ],
    "homepage": "https://github.com/jeffersonsimaogoncalves/laravel-device-tracking",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ivano Matteo",
            "email": "ivanomatteo@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Jefferson Simão Gonçalves",
            "email": "gerson.simao.92@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0",
        "hisorange/browser-detect": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "IvanoMatteo\\LaravelDeviceTracking\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IvanoMatteo\\LaravelDeviceTracking\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "IvanoMatteo\\LaravelDeviceTracking\\LaravelDeviceTrackingServiceProvider"
            ],
            "aliases": {
                "DeviceTracker": "IvanoMatteo\\LaravelDeviceTracking\\LaravelDeviceTrackingFacade"
            }
        }
    }
}
